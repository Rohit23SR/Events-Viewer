# Events Viewer Environment Configuration
# Copy this file to .env.local for local development

# API Configuration
VITE_API_URL=https://teg-coding-challenge.s3.ap-southeast-2.amazonaws.com/events/event-data.json

# CORS Proxy (used in production when direct API access is blocked)
VITE_CORS_PROXY_URL=https://corsproxy.io/?

# Feature Flags
VITE_ENABLE_DARK_MODE=true
VITE_ENABLE_SEARCH=true
VITE_ENABLE_SORTING=true

# Analytics (optional)
# VITE_ANALYTICS_ID=your-analytics-id
